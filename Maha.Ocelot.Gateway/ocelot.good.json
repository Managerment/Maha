{ //创建多个配置文件分别对不同项目进行配置，Ocelot自动合并
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/good/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8002
        },
        {
          "Host": "localhost",
          "Port": 8003
        }
      ],
      "UpstreamPathTemplate": "/good/{id}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
